{
  "name": "excellent-child",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@parcel/transformer-sass": "^2.8.3",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "husky": "^8.0.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "lint-staged": "^13.2.0",
    "stylelint": "^15.4.0",
    "stylelint-config-clean-order": "^5.0.1",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^4.6.0",
    "parcel": "^2.8.3"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "postcss-loader": "^7.2.4",
    "postcss-modules": "^6.0.0"
  },
  "scripts": {
    "build": "parcel build ./assets/*.js --public-url ./dist",
    "dev": "parcel watch ./assets/*.js --public-url ./dist",
    "lint": "run-s -cl lint:scripts lint:styles",
    "lint:scripts": "eslint --color --fix --ext .jsx,.js ./**/*.{js,jsx}",
    "lint:styles": "stylelint --color --fix ./**/*.scss",
    "husky": "npx husky-init && yarn",
    "prepare": "cd ../../../ && husky install wp-content/themes/excellent-child/.husky"
  },
  "browserslist": [
    "> 0.5%, last 2 versions, not dead"
  ],
  "targets": {
    "main": false
  }

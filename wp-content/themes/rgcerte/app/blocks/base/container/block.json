{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "version": "1.0.0",
  "name": "makersolucoes/container",
  "title": "Container",
  "description": "Este componente de container!",
  "keywords": [
    "container",
    "box"
  ],
  "attributes": {
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "template": {
      "type": "array"
    },
    "allowedBlocks": {
      "type": "array"
    }
  },
  "supports": {
    "color": {
      "text": true,
      "background": true,
      "gradients": true
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
			"width": true,
			"style": true,
      "__experimentalDefaultControls": {
        "color": true
      }
    }
  },
  "textdomain": "boilerplate",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}

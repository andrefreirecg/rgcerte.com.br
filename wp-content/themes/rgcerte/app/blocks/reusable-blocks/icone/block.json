{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "version": "1.0.0",
  "name": "makersolucoes/icone",
  "title": "Icone",
  "icon": "image-filter",
  "category": "ms_blocks",
  "description": "Exibe um Ã­cone com base em uma imagem da galeria",
  "keywords": [
    "icon"
  ],
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "checked": {
      "type": "boolean",
      "default": false
    }
  },
  "providesContext": {
    "contextLabelChecked": "checked"
  },
  "supports": {
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "__experimentalBorder": {
      "color": true,
      "width": true,
      "style": true,
      "radius": true,
      "__experimentalDefaultControls": {
        "color": true
      }
    },
    "align": true,
    "layout": {
      "allowVerticalAlignment": true
    }
  },
  "textdomain": "boilerplate",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}

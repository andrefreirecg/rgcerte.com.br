{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "version": "1.0.0",
  "name": "makersolucoes/hero",
  "title": "Hero",
  "icon": "cover-image",
  "category": "ms_blocks",
  "description": "Exibe o hero principal",
  "keywords": [
    "hero"
  ],
  "attributes": {
    "maxWidth": "0",
    "preview": {
      "type": "boolean",
      "default": false
    },
    "checked": {
      "type": "boolean",
      "default": false
    }
  },
  "providesContext": {
    "contextLabelChecked": "checked"
  },
  "supports": {
    "html": false,
    "color": {
      "text": true,
      "background": true,
      "gradients": true
    }
  },
  "textdomain": "boilerplate",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"version": "1.0.0",
	"name": "makersolucoes/accordion-body",
	"title": "Body-Acordeon",
	"category": "ms_blocks",
	"description": "Exibe um componente de Accordion Body",
	"icon": "table-row-after",
	"keywords": [
		"icon"
	],
	"attributes": {
		"open": {
			"type": "array",
			"default": []
		},
		"buttonsStyle": {
			"type": "string",
			"default": ""
		}
	},
	"providesContext": {
		"contextLabelChecked": "checked"
	},
	"supports": {
		"html": false,
		"alignWide": true,
		"color": {
			"text": true,
			"background": true,
			"gradients": true
		},
		"width": true,
		"__experimentalBorder": {
			"color": true,
			"width": true,
			"style": true,
			"__experimentalDefaultControls": {
			  "color": true
			}
		},
		"innerBlocks": true

	},
	"textdomain": "boilerplate",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"}

{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "version": "1.0.0",
  "name": "makersolucoes/divider",
  "title": "Divisor",
  "category": "ms_blocks",
  "description": "Exibe um divider",
  "icon": "minus",
  "keywords": [
    "icon"
  ],
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "checked": {
      "type": "boolean",
      "default": false
    },
    "maxWidth": {
      "type": "number",
      "default": 50
    },
    "borderRadius": {
      "type": "number",
      "default": 50
    },
    "minHeight": {
      "type": "number",
      "default": 5
    }
  },
  "providesContext": {
    "contextLabelChecked": "checked"
  },
  "supports": {
    "html": false,
    "alignWide": true,
    "color": {
      "text": false,
      "background": true,
      "gradients": true
    },
    "width": true
  },
  "textdomain": "boilerplate",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}

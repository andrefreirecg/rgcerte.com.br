{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "version": "1.0.0",
  "name": "makersolucoes/card",
  "title": "Card",
  "category": "ms_blocks",
  "description": "Exibe uma card com estilo principal",
  "keywords": [
    "card"
  ],
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "linkAttribute": {
      "type": "string",
      "default": ""
    },
    "maxWidth": {
      "type": "number",
      "default": null
    },
    "checked": {
      "type": "boolean",
      "default": false
    }
  },
  "providesContext": {
    "contextLabelChecked": "checked"
  },
  "supports": {
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "color": {
      "text": true,
      "background": true,
      "gradients": true
    }
  },
  "textdomain": "boilerplate",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}